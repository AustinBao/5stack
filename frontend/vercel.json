{
  "rewrites": [
    {
      "source": "/auth/(.*)",
      "destination": "https://5stack.up.railway.app/auth/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://5stack.up.railway.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
